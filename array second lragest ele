#include<iostream>
using namespace std;
int main()
{
    int n , arr[100];
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        
    }
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
        
    }
    sort(arr,arr+n);
    cout<<"SEcond largest elemrnt is : "<<arr[n-2]; 

    
    return 0;
}
